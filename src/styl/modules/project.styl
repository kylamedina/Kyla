.wolf-item.wfull
	.wolf-item-inner, img
		float none
		display block
		margin-left auto
		margin-right auto

.browser-shadow
	box-shadow 0px 5px 30px rgba(#000,0.1)
	border-bottom-right-radius 3px
	border-bottom-left-radius 3px
	border-top 1px solid #f0f0f0
.browser-chrome
	&:after
		content ''
		display block
		height px(30)
		position absolute
		top px(-30)
		left 0
		right 0
		width 100%
		z-index 999
		background white url('../img/dots.png') no-repeat
		background-position px(10) 50%
		background-size auto px(6)
.hero--artnet
	.hero-overlay
		background-color rgba(#000,0.4)
	.parallax-image
		background-position: 0% 0% !important
		
.body--artnet
	.gallery li
		border 1px solid #f0f0f0

