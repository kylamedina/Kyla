doctype html
html.layout
  head

    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0')
    meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
    title Kyla Medina
    script.
      function aload(t){"use strict";t=t||window.document.querySelectorAll("[data-aload]"),void 0===t.length&&(t=[t]);var a,e=0,r=t.length;for(e;r>e;e+=1)a=t[e],a["LINK"!==a.tagName?"src":"href"]=a.getAttribute("data-aload"),a.removeAttribute("data-aload");return t}
    style.
      [data-aload] { background-image: none !important; }
    //- link(href='http://fonts.googleapis.com/css?family=Montserrat:400,700', rel='stylesheet', type='text/css')
    link(href='http://fonts.googleapis.com/css?family=Maven+Pro:400,700,900', rel='stylesheet', type='text/css')
    link(href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900', rel='stylesheet', type='text/css')
    //- link(href='http://fonts.googleapis.com/css?family=Josefin+Sans:400,700', rel='stylesheet', type='text/css')
    link(href='static/favicons/favIcon_48x48.ico' rel='shortcut icon' type='image/x-icon')

    link#font-style-css(rel='stylesheet', href='./css/font.css', type='text/css', media='all')
    link#default-style-css(rel='stylesheet', href='./css/style.css', type='text/css', media='all')
    link#vendor-css(rel='stylesheet', href='./css/vendor.css', type='text/css', media='all')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/headjs/1.0.3/head.min.js' type="text/javascript")
    block head
  body#top
    //- img.video-cover(data-aload='bg3.gif')
    .hidden
     include symbols.svg
    //- PAGELOADER
    #page-loader.page-loader
      .page-loader-inner.no-mobile-video
        //- img.loader(src='loading.gif')
        video.loader(preload="auto" autoplay loop poster='data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7')
          source(src='./static/loading_gradient.mp4')
        //- .loader
        //-   strong Loading
    block content
    include ../includes/modal.jade
    include ../includes/footer.jade
    script.
      head.load({ app: "./js/app.js" }, function() {
          window.onload = function () {
            aload();
          };
      });

      
    block footer
    browsersync
