extends layouts/sublayout

block content
  //- include includes/triangles.jade
  - var headerLight = true
  include includes/header.jade
  #page-content.page-hwc
    //- section#hero.hero-full.parallax-section.text-light(data-parallax-image='../img/hwc/hwc_02-large.jpg')
    section#hero.hero-full.parallax-section.text-light(data-parallax-image='../img/projects/hwc-large.jpg')
      .page-title
        h5.alttitle Design, Development, and Photography
        h1
          strong Hard Workers Club
      //- a#scroll-down(href='#') Scroll Down
    section#page-body
      //- .wrapper
      //-   .wolf-grid.clearfix(data-randomhorizontaloffset='false')
      //-     .wolf-item.wfull
      //-       .wolf-item-inner
      //-         .wolf-media.portfolio-media.browser-chrome
      //-           img.browser-shadow(src='../img/hwc/hwc_01.png', alt='SEO NAME')
      .wrapper.xtra-wide
        
        ul.gallery.gallery-col1
          li.has-animation
            .browser-chrome.is-large
              img.browser-shadow(data-aload="../img/hwc/hwc_01-medium.jpg" srcset="../img/hwc/hwc_01-small.jpg 640w, ../img/hwc/hwc_01-medium.jpg 800w, ../img/hwc/hwc_01-large.jpg 1800w" alt="")
      .wrapper-mini
        .spacer.spacer-big
        h6
          strong Project Details
        p
          | Hard Workers Club is my photography website that I designed on Tumblr for personal work. The photos of a girl falling or hanging from things is me. I do all my own stunts. Outside of personal work, I've Below is my Photography CV.
        h6
          strong Exhibitions
        ul.cv-list
          li New Landscapes, Drewelowe Gallery, Iowa City, IA, 2011
          li 
            a(href='http://www.occca.org/2011-10.html') You First
            | , Orange County Center for Contemporary Art, Santa Ana, 2011
          li 
            a(href='http://www.foto8.com/live/foto8-summershow-2011/') Foto8 Summershow 2011
            | , London, England
        h6
          strong Press
        ul.cv-list
          li 
            a(href='http://jenbekmanprojects.tumblr.com/post/5842794474/untitled-2011-by-kyla-medina-hhs-contender') 20x200 
            | , HHS! Contender: Kyla Medina, 2011
          li 
            a(href='http://readcereal.com/forest-gallery-selects/') Cereal Magazine
            | , Forest, Vol. 4, 2013
          li 
            a(href='http://issuu.com/hesa_inprint/docs/hesa_inprint_06') HESA Magazine
            | , Transportation, Issue 6, 2011
          li 
            a(href='http://www.amazon.com/Unreality-UFORA-Nerealnost-UFORY-Contemporary/dp/1500473472/ref=sr_1_1?ie=UTF8&qid=1430236267&sr=8-1&keywords=the+unreality+of+UFORA') The Unreality of UFORA / Nerealnost' UFORY: Notes on Contemporary Art
            | , Rob Mintz, Dalibor Polivka, 2013
        h6
          strong Collections
        ul.cv-list
          li 
            a(href='http://www.cranekalmanbrighton.com/exhibitions/2011_foto8/image_10.html') Crane Kalman Gallery
            | , Brighton, England
      div.wrapper-xtrawide
        .spacer.spacer-big
        .wolf-grid.clearfix(data-randomhorizontaloffset='true' data-mouseparallax='true')
          .wolf-item
            .wolf-item-inner
              .wolf-media.portfolio-media
                a(href='../img/hwc/hwc_03-large.jpg', data-rel='lightcase:hwc')
                  img.img-shadow(data-aload='hwc_03-medium.jpg' srcset="../img/hwc/hwc_03-small.jpg 640w, ../img/hwc/hwc_03-medium.jpg 800w, ../img/hwc/hwc_03-large.jpg 1280w" alt='Photography Work')
          .wolf-item
            .wolf-item-inner
              .wolf-media.portfolio-media
                a(href='../img/hwc/hwc_02-large.jpg', data-rel='lightcase:hwc')
                  img.img-shadow(data-aload='hwc_02-medium.jpg' srcset="../img/hwc/hwc_02-small.jpg 640w, ../img/hwc/hwc_02-medium.jpg 800w, ../img/hwc/hwc_02-large.jpg 1280w" alt='Photography Work')
          .wolf-item
            .wolf-item-inner
              .wolf-media.portfolio-media
                a(href='../img/hwc/hwc_04-large.jpg', data-rel='lightcase:hwc')
                  img.img-shadow(data-aload='../img/hwc/hwc_04-medium.jpg' srcset="../img/hwc/hwc_04-small.jpg 640w, ../img/hwc/hwc_04-medium.jpg 800w, ../img/hwc/hwc_04-large.jpg 1500w" alt='Photography Work')
          .wolf-item
            .wolf-item-inner
              .wolf-media.portfolio-media
                a(href='../img/hwc/hwc_05-large.jpg', data-rel='lightcase:hwc')
                 img.img-shadow(data-aload='../img/hwc/hwc_05-medium.jpg' srcset="../img/hwc/hwc_05-small.jpg 640w, ../img/hwc/hwc_05-medium.jpg 800w, ../img/hwc/hwc_05-large.jpg 1600w" alt='Photography Work')
          .wolf-item
            .wolf-item-inner
              .wolf-media.portfolio-media
                a(href='../img/hwc/hwc_07-large.jpg', data-rel='lightcase:hwc')
                  img.img-shadow(data-aload='../img/hwc/hwc_07-medium.jpg' srcset="../img/hwc/hwc_07-small.jpg 640w, ../img/hwc/hwc_07-medium.jpg 800w, ../img/hwc/hwc_07-large.jpg 1280w" alt='Photography Work')
          .wolf-item
            .wolf-item-inner
              .wolf-media.portfolio-media
                a(href='../img/hwc/hwc_06-large.jpg', data-rel='lightcase:hwc')
                 img.img-shadow(data-aload='../img/hwc/hwc_06-medium.jpg' srcset="../img/hwc/hwc_06-small.jpg 640w, ../img/hwc/hwc_06-large.jpg 853w" alt='Photography Work')
          
          .wolf-item
            .wolf-item-inner
              .wolf-media.portfolio-media
                a(href='../img/hwc/hwc_08-large.jpg', data-rel='lightcase:hwc')
                  img.img-shadow(data-aload='../img/hwc/hwc_08-medium.jpg' srcset="../img/hwc/hwc_08-small.jpg 640w, ../img/hwc/hwc_08-medium.jpg 800w, ../img/hwc/hwc_08-large.jpg 1280w" alt='Photography Work')
       

      .wrapper
        ul.single-pagination
          li.prev
            a(href='../success-academy/' alt='Success Academy')
              img(src='../img/sa/sa_02-small.jpg', alt='prev')
              span Previous Project
          li.backtoworks
            a(href='../') All Projects
          li.next
            a(href='../akc'  alt='AKC')
              img(src='../img/projects/akc-small.png', alt='next')
              span Next Project
