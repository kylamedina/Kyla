extends layouts/layout

block content
  section#hero.hero-full.parallax-section.text-light(data-parallax-image='img/hwc/hwc_02.jpg')
    .page-title
      h1
        strong Hard Workers Club
    a#scroll-down(href='#') Scroll Down
  section#page-body
