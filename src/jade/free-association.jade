						a(href='weather-channel.html' style='background:url("img/twc/rain.gif");background-size:cover;background-position: 50% 100%;')
							img(src='img/projects/twc2.png', alt='prev')
							span Previous Project
					li.backtoworks
						a(href='/') Back To Works
					li.next
						a(href='mkbl.html')
							img(src='img/projects/mkbl-small.jpg', alt='next')
							span Next Project

block footer
	script
		:coffee-script
			head.load { velocity: '//cdnjs.cloudflare.com/ajax/libs/velocity/1.2.3/velocity.min.js' }, ->
				$shape = $('.svg-shape')
				$circle = $('#circle')
				$triangle = $('#triangle')
				$line = $('#line')
				$line2 = $('#line2')
				$bg_s1 = $('#linearGradient-bg1 .stop-1')
				$bg_s2 = $('#linearGradient-bg1 .stop-2')
				#lineHeight = ($('.hero-big').width())

				resizeTimer = undefined
				
				window.theme1FirstTime = true

				bgColorGradient = (color1,color2) ->
					$bg_s1.attr('stop-color', color1)
					$bg_s2.attr('stop-color', color2)

				theme1 = () ->
					ww = $(window).width()/1.2
					ww2 = $(window).width()/2
					n = $(window).width() - ($circle.height()/2)
					m = $(window).width()/3
					o = $(window).width()/4
					if $(window).width() > $(window).height()
						lineHeight = $(window).width()
					else
						lineHeight = $(window).height()
					if window.theme1FirstTime
						$triangle.velocity {				
							height: 462
							width: 462
						},
							duration: 3000
							easing: 'ease'
						$line.velocity {
							height: lineHeight*1.1
							left: ww*1.3
						},
							duration: 3000
							easing: 'ease'
						$line.find('rect').velocity {
							height: lineHeight*1.1
						},
							duration: 3000
							easing: 'ease'
						$line2.velocity {
							opacity: 0
						},
							duration: 1000
							easing: 'ease'
					$circle.velocity {
						left: n
						top: '173px'
						rotateZ: '0deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$line.velocity {
						top: '-10px'
						left: o
						rotateZ: '35deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$triangle.velocity {
						left: -o
						bottom: '-30px'
						rotateZ: '-45deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$triangle.find('g').attr('stroke-width','5')
					bgColorGradient('#409CF9','#37F6E1')
					$('.hero-big').removeClass('theme-green theme-purple').addClass('theme-blue')
					window.theme1FirstTime = false
				theme2 = () ->
					ww = $(window).width()
					n = $circle.height()*2
					m = $(window).width()/2
					if $(window).width() > 1600
						m = $(window).width()/1.6
					o = $(window).width()-$triangle.width()
					
					$circle.velocity {
						left: ww - $circle.width()
						top: '100px'
						rotateZ: '-10deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$line.velocity {
						left: m/1.5
						rotateZ: '-35deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$triangle.velocity {
						left: m/2
						bottom: '-10px'
						rotateZ: '50deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					bgColorGradient('#BC68FF','#4B88FA')
					$('.hero-big').removeClass('theme-blue theme-green').addClass('theme-purple')

				theme3 = () ->
					ww = $(window).width()
					n = $(window).width() - ($circle.height())
					m = $('.hero-big').height()/2
					o = $(window).width()*0.9
					p = $('.hero-big').height() - ($triangle.width()/2.2)
					if $(window).height() > 800
						p = $('.hero-big').height()/3
					q = (-$(window).width()/3)
					
					$circle.velocity {
						left: 0
						top: m
						rotateZ: '240deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$line.velocity {
						left: o
						rotateZ: '35deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$triangle.velocity {
						left: q
						bottom: p
						rotateZ: '43deg'
					},
						duration: 3000
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					bgColorGradient('#00F0C7','#ADFFA5')
					$('.hero-big').removeClass('theme-blue theme-purple').addClass('theme-green')

				logoPosition = () ->
					shapeStyles = ''
					# $line.clone().attr('id', 'line2').appendTo('header.header')
					ww = $(window).width()
					ww2 = $(window).width()/2
					wh = $('.hero-big').height() 
					wh2 = $('.hero-big').height()/2
					tw = $triangle.width()
					th = $triangle.height()
					cw = $circle.height()*1.6
					circleHorizontalCenter = -ww2 + 226
					circleVerticalCenter = wh2 - 226
					triangleHorizontalCenter = ww2 - (tw/2)
					triangleVerticalCenter = wh2 - 130
					lineTop1 = wh2 - 62
					lineTop2 = wh2 + 50
					lineLeft1 = ww2 + 65
					lineLeft2 = ww2 + 210

					$circle.velocity {
						left: -circleHorizontalCenter
						top: (circleVerticalCenter+25)
						height: 452
						width: 452
						rotateZ: '0deg'
					},
						duration: 2900
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$line.velocity {
						left: lineLeft1
						top: lineTop1
						height: 262
						rotateZ: '90deg'
					},
						duration: 2900
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$line2.velocity {
						left: lineLeft2
						top: lineTop2
						height: 345
						rotateZ: '90deg'
					},
						duration: 2900
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$triangle.velocity {
						height: $triangle.height()/1.5
						width: $triangle.width()/1.5
						left: triangleHorizontalCenter
						bottom: triangleVerticalCenter
						rotateZ: '0deg'
					},
						duration: 2900
						easing: 'ease'
						queue: ''
						begin: undefined
						progress: undefined
						complete: undefined
						display: undefined
						visibility: undefined
						loop: false
						delay: false
						mobileHA: true
					$triangle.find('g').attr('stroke-width','7')
					$('.svg-shape').css('opacity','1')


				showLogo = () ->
					$('.page-title').on 'mouseover', ->
						if !head.touch
							clearInterval(window.themeInteraval)
							logoPosition()
							$('.page-title').css 'opacity', '0.2'
							$('#line2').css 'opacity', '1'
					$('.page-title').on 'mouseout', ->
						if !head.touch
							# $('.page-title').css 'opacity', '1'
							$('.svg-shape').not('#line2').css('opacity','1')
							$('#line2').css 'opacity', '0'
							switchHeader()
							window.themeInteraval

				#$(window).on 'resize', (e) ->
				#	clearTimeout resizeTimer
				#	resizeTimer = setTimeout((->
				#		#lineHeight = ($('.hero-big').width())
				#		#findLineHeight()
				#		return
				#	), 250)
				#	return

				$(document).on 'ready', (event) ->
					$('.svg-shape').not('#line2').css('opacity','1')
					#theme3()
					if !head.mobile
						logoPosition()
						window.i = 0
						window.themeInteraval = setInterval (->
							switch window.i
								when 0 then theme1() 
								when 1 then theme2() 
								when 2 then theme3() 
							window.i++
							if window.i > 2
								window.i = 0
							), 3000
					else
						theme1()